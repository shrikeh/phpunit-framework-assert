{
    "name": "phpunit/phpunit-framework-assert",
    "description": "Assertion library for PHPUnit",
    "type": "library",
    "keywords": [
        "phpunit",
        "xunit",
        "testing",
        "assertions",
        "constraints"
    ],
    "homepage": "https://phpunit.de/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/sebastianbergmann/phpunit-framework-assert/issues"
    },
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-xml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4.1"
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "suggest": {
        "ext-xdebug": "*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "files": [
            "src/Framework/Assert/Functions.php",
            "tests/_files/CoveredFunction.php"
        ]
    }
}
